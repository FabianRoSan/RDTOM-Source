Listen 8080

<VirtualHost *:8080>
  LoadModule php5_module /home/ubuntu/.phpenv/versions/5.4.4/libexec/libphp5.so

  DocumentRoot "/home/ubuntu/RDTOM-Source/html"
  ServerName rdtom
  DirectoryIndex index.php

  <FilesMatch \.php$>
    SetHandler application/x-httpd-php
  </FilesMatch>

  <Directory />
Allow from all
    Require all granted
    AllowOverride All
  </Directory>
</VirtualHost>
