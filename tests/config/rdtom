Listen 8080

  LoadModule php5_module /home/ubuntu/.phpenv/versions/PHP_VERSION/libexec/libphp5.so

  DocumentRoot "/home/ubuntu/RDTOM-Source/html"
  ServerName rdtom
  DirectoryIndex index.php

  <FilesMatch \.php$>
    SetHandler application/x-httpd-php
  </FilesMatch>

  <Directory "/home/ubuntu/RDTOM-Source/html">
    Require all granted 
    AllowOverride All
    Allow from All
  </Directory>